                                $('#addtofav').addClass("bi bi-heart-fill");
                                $('#addtofav').css("color","red");
                            }
                            else{
                                $('#addtofav').css("color","black");
                                $('#addtofav').removeClass();
                                 $('#addtofav').addClass("bi bi-heart");
                            }
                        },
                        error: function (request, error){
                            console.log(error);
                        }
                    })
